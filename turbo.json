{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "DIRECT_DATABASE_URL",
        "NEXT_PUBLIC_API_URI",
        "NEXT_PUBLIC_LANDING_URI",
        "NEXT_PUBLIC_DASHBOARD_URI",
        "NEXT_PUBLIC_BACKOFFICE_URI",
        "NEXT_PUBLIC_WIKI"
      ],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "sitemap": {
      "dependsOn": ["build"],
      "outputs": ["public/robots.txt", "public/sitemap*.xml"]
    },
    "deploy": {
      "dependsOn": ["build", "sitemap"],
      "outputs": []
    },
    "start": {
      "dependsOn": ["^start"],
      "env": [
        "NODE_ENV",
        "JWT_SECRET",
        "DATABASE_URL",
        "DIRECT_DATABASE_URL",
        "REDIS_URL",
        "API_URI",
        "DASHBOARD_URI",
        "LANDING_URI",
        "AWS_CLOUDFRONT_DISTRIBUTION_ID",
        "AWS_S3_ACCESS_KEY_ID",
        "AWS_S3_ACCESS_KEY_SECRET",
        "AWS_S3_BUCKET",
        "AWS_SES_REGION",
        "AWS_SES_ACCESS_KEY_ID",
        "AWS_SES_SECRET_ACCESS_KEY",
        "SES_CONFIGURATION_SET",
        "SES_CONFIGURATION_SET_NO_TRACKING",
        "GITHUB_OAUTH_CLIENT",
        "GITHUB_OAUTH_SECRET",
        "GOOGLE_OAUTH_CLIENT",
        "GOOGLE_OAUTH_SECRET",
        "STRIPE_SK",
        "STRIPE_WEBHOOK_SECRET",
        "STRIPE_PRICE_ONBOARDING",
        "STRIPE_PRICE_EMAIL_USAGE",
        "STRIPE_METER_EVENT_NAME",
        "PLUNK_API_KEY",
        "NEXT_PUBLIC_API_URI",
        "NEXT_PUBLIC_LANDING_URI",
        "NEXT_PUBLIC_DASHBOARD_URI",
        "NEXT_PUBLIC_BACKOFFICE_URI",
        "NEXT_PUBLIC_WIKI"
      ],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "cache": false,
      "persistent": true
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "NODE_ENV",
        "JWT_SECRET",
        "DATABASE_URL",
        "DIRECT_DATABASE_URL",
        "REDIS_URL",
        "API_URI",
        "DASHBOARD_URI",
        "LANDING_URI",
        "AWS_CLOUDFRONT_DISTRIBUTION_ID",
        "AWS_S3_ACCESS_KEY_ID",
        "AWS_S3_ACCESS_KEY_SECRET",
        "AWS_S3_BUCKET",
        "AWS_SES_REGION",
        "AWS_SES_ACCESS_KEY_ID",
        "AWS_SES_SECRET_ACCESS_KEY",
        "SES_CONFIGURATION_SET",
        "SES_CONFIGURATION_SET_NO_TRACKING",
        "GITHUB_OAUTH_CLIENT",
        "GITHUB_OAUTH_SECRET",
        "GOOGLE_OAUTH_CLIENT",
        "GOOGLE_OAUTH_SECRET",
        "STRIPE_SK",
        "STRIPE_WEBHOOK_SECRET",
        "STRIPE_PRICE_ONBOARDING",
        "STRIPE_PRICE_EMAIL_USAGE",
        "STRIPE_METER_EVENT_NAME",
        "PLUNK_API_KEY",
        "NEXT_PUBLIC_API_URI",
        "NEXT_PUBLIC_LANDING_URI",
        "NEXT_PUBLIC_DASHBOARD_URI",
        "NEXT_PUBLIC_BACKOFFICE_URI",
        "NEXT_PUBLIC_WIKI"
      ]
    },
    "dev:worker": {
      "cache": false,
      "persistent": true,
      "env": [
        "NODE_ENV",
        "JWT_SECRET",
        "DATABASE_URL",
        "DIRECT_DATABASE_URL",
        "REDIS_URL",
        "API_URI",
        "DASHBOARD_URI",
        "LANDING_URI",
        "AWS_CLOUDFRONT_DISTRIBUTION_ID",
        "AWS_S3_ACCESS_KEY_ID",
        "AWS_S3_ACCESS_KEY_SECRET",
        "AWS_S3_BUCKET",
        "AWS_SES_REGION",
        "AWS_SES_ACCESS_KEY_ID",
        "AWS_SES_SECRET_ACCESS_KEY",
        "SES_CONFIGURATION_SET",
        "SES_CONFIGURATION_SET_NO_TRACKING",
        "GITHUB_OAUTH_CLIENT",
        "GITHUB_OAUTH_SECRET",
        "GOOGLE_OAUTH_CLIENT",
        "GOOGLE_OAUTH_SECRET",
        "STRIPE_SK",
        "STRIPE_WEBHOOK_SECRET",
        "STRIPE_PRICE_ONBOARDING",
        "STRIPE_PRICE_EMAIL_USAGE",
        "STRIPE_METER_EVENT_NAME",
        "PLUNK_API_KEY",
        "NEXT_PUBLIC_API_URI",
        "NEXT_PUBLIC_LANDING_URI",
        "NEXT_PUBLIC_DASHBOARD_URI",
        "NEXT_PUBLIC_BACKOFFICE_URI",
        "NEXT_PUBLIC_WIKI"
      ]
    },
    "clean": {
      "cache": false,
      "persistent": true
    }
  }
}
